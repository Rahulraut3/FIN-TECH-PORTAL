
@{
    ViewData["Title"] = "ChangePassForFixed";
    Layout = "~/Views/Shared/_FixedCustomerLayout.cshtml";
}
<h5 style="color:red;">@ViewBag.ExcError</h5>

<hr />  
<div class="row">
    <div class="col-md-4 offset-xl-1">

        <form action="ChangePassForFixed" method="post">
            
           <div class="form-outline mb-4">
              <label class="form-label"> New Password</label>
            <input type="text" id="Password" name="Password" class="form-control form-control-lg"
              placeholder="Enter here" />
          </div>
          <div class="text-center text-lg-start mt-4 pt-2">
            <input type="submit" class="btn btn-primary btn-lg" value="Change"
              style="padding-left: 2.5rem; padding-right: 2.5rem;"/>
          </div>
        </form>
        <br/>
    </div>
</div>


