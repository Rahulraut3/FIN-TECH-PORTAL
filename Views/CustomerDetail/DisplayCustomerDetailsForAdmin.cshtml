@model IEnumerable<IBS_DALayer.Models.CustomerAllDetail>

@{
    ViewData["Title"] = "DisplayCustomerDetailsForAdmin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h5 style="color:red;">@ViewBag.ExcError</h5>

<table class="table table-primary table-hover ">
    <thead class="thead-dark">
        <tr>
             <th>
                Acc Number
            </th>
            <th>
                IFSC Code
            </th>
            <th>
               Acc Type
            </th>
            
            <th>
                Creation Date
            </th>
            <th>
                Customer Name
            </th>
            
            <th>
                Ph Number
            </th>

            <th>
               Aadhar
            </th>
           
            <th>
                Nominee Name
            </th>
            
            <th>
                Nom Phone
            </th>
            
           
            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AccountNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IFSCCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AccountType)
            </td>
            
            <td>
                @Html.DisplayFor(modelItem => item.AccountCreationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustomerName)
            </td>
            
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumber)
            </td>
            
            
            <td>
                @Html.DisplayFor(modelItem => item.Aadhar)
            </td>
            
            <td>
                @Html.DisplayFor(modelItem => item.NomineeName)
            </td>
            
            <td>
                @Html.DisplayFor(modelItem => item.NomPhoneNumber)
            </td>
            
            
            
        </tr>
}
    </tbody>
</table>